#include<iostream>
using namespace std;
int main()
{
    int x,y;
    cin>>x;
    y=(x*x)%10;
    if(x<=10)
    {
        if(x==y){
            cout<<"Automorphic Number";
        }
        else
        {
            cout<<"Not Automorphic Number";
        }
    }
    else
    cout<<"Invalid input";
}
